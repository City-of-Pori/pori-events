{
    "name": "wunderio/pori_events",
    "description": "Drupal 9 based Pori Events Composer Project",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "package",
            "package": {
                "name": "kenwheeler/slick",
                "version": "1.8.1",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/kenwheeler/slick/archive/refs/tags/v1.8.1.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "dinbror/blazy",
                "version": "1.8.2",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/dinbror/blazy/archive/1.8.2.zip",
                    "type": "zip"
                }
            }
        }
    ],
    "require": {
        "composer/installers": "^2.2",
        "cweagans/composer-patches": "^1.7",
        "drush/drush": "^11.1",
        "webflo/drupal-finder": "^1.0.0",
        "drupal/warden": "^3.0@RC",
        "drupal/simplei": "^1.1",
        "wunderio/drupal-ping": "^2.3",
        "drupal/address": "^1.10",
        "drupal/elasticsearch_helper": "7.x-dev",
        "drupal/admin_toolbar": "^3.1",
        "drupal/coffee": "^1.0",
        "ccampbell/chromephp": "^4.1",
        "drupal/migrate_plus": "^6.0",
        "drupal/migrate_tools": "^6.0",
        "drupal/taxonomy_multidelete_terms": "^1.3",
        "drupal/structured_data": "^1.0",
        "drupal/imagecache_external": "^3.0",
        "drupal/entityqueue": "^1.0",
        "drupal/slick": "^2.6",
        "drupal/slick_views": "^2.6",
        "drupal/fontawesome": "^2.23",
        "drupal/twig_tweak": "^3.1",
        "drupal/google_analytics": "^3.1",
        "kenwheeler/slick": "^1.8",
        "dinbror/blazy": "^1.8",
        "drupal/conditional_fields": "^4.0@alpha",
        "drupal/migrate_source_jsonpath": "1.x-dev",
        "drupal/context": "^4.1",
        "drupal/empty_page": "^3",
        "drupal/simple_gmap": "^3.0",
        "drupal/pathauto": "^1.10",
        "drupal/core-recommended": "^9.4",
        "drupal/core-composer-scaffold": "^9.4",
        "drupal/blazy": "^2.13",
        "elasticsearch/elasticsearch": "^7.11",
        "drupal/ctools": "^4.0",
        "drupal/google_tag": "^1.5",
        "drupal/datalayer": "^1.0@beta"
    },
    "require-dev": {
        "drupal/config_devel": "^1.2",
        "drupal/stage_file_proxy": "^1.2",
        "drupal/twig_xdebug": "^1.0",
        "drupal/devel": "^4.1"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "config": {
            "sort-packages": true
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "./web"
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ]
        },
        "dropin-paths": {
            "web/": [
                "type:web-dropin",
                "package:wunderio/drupal-ping:_ping.php"
            ]
        },
        "patches": {
            "drupal/datalayer": {
                "Drupal 9 compatibility: Exception page because of incorrect path.alias_manager service name": "https://www.drupal.org/files/issues/2020-09-18/3171896-path_alias-manager.patch"
            },
            "drupal/migrate_source_jsonpath": {
                "https://www.drupal.org/project/migrate_source_jsonpath/issues/3277645: Incompatible migrate_plus Json methods after upgrate to migrate_plus ^6.0": "https://www.drupal.org/files/issues/2022-04-28/3277645-incompatible-migrateplus-json-2022-04-28-001.patch"
            },
            "drupal/twig_tweak": {
                "https://www.drupal.org/project/twig_tweak/issues/3137994: imageStyle should return original image path as a fallback": "https://www.drupal.org/files/issues/2021-10-15/3137994-8-imagestyle-fallback-path.patch"
            }
        }
    },
    "config": {
        "discard-changes": true,
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "koodimonni/composer-dropin-installer": true,
            "drupal/core-composer-scaffold": true
        }
    }
}
